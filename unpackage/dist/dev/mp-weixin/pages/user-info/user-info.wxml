<view><uni-list-item vue-id="1" title="头像" bind:__l="__l" vue-slots="{{['right']}}"><view slot="right"><image class="rounded-circle" style="height:90rpx;width:90rpx;" src="{{user.avatar}}" data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" bindtap="__e"></image></view></uni-list-item><uni-list-item vue-id="2" title="用户名" data-event-opts="{{[['^click',[['toeditUsername']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['right']}}"><view slot="right">{{''+user.username+''}}</view></uni-list-item><view hidden="{{!(editusername)}}" class="uni-common-mt"><view class="uni-form-item uni-column"><view class="title text-center">更改用户名</view><input class="uni-input border mx-3" style="height:75rpx;" focus placeholder="输入用户名" data-event-opts="{{[['input',[['update',['$event']]]]]}}" value="{{user.username2}}" bindinput="__e"/></view><view class="p-3 d-flex"><view class="text-center w-100 flex j-center text-white font-md rounded py-2" style="background-color:#00BFFF;" hover-class="main-bg-hover-color" data-event-opts="{{[['tap',[['canceleditusername',['$event']]]]]}}" bindtap="__e">取 消</view><view class="text-center w-100 flex j-center main-bg-color text-white font-md rounded py-2" hover-class="main-bg-hover-color" data-event-opts="{{[['tap',[['overeidtusername',['$event']]]]]}}" bindtap="__e">确 定</view></view></view><divider vue-id="3" bind:__l="__l"></divider><view class="py-3 px-5 text-light-muted"><view class="text-center w-100 main-bg-color text-white font-md rounded py-2" hover-class="main-bg-hover-color" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">确 定</view></view><divider vue-id="4" bind:__l="__l"></divider><uni-list-item vue-id="5" title="修改密码" data-event-opts="{{[['^click',[['tochangepassword']]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item><uni-list-item vue-id="6" title="手机号(不可更改)" bind:__l="__l" vue-slots="{{['right']}}"><view slot="right">{{''+userInfo.phone+''}}</view></uni-list-item></view>